{% if item.acf_fc_layout == 'team_block' %}
  <section class="team">
    <h2 class="section-title">{{ item.title }}</h2>
    <p class="text">{{ item.text }}</p>
      {% for item in item.team_member %}
        <div class="profile">
          <img src="{{ Image(item.image).src }}">
          <div class="details">
            <h3>{{ item.name }}</h3>
            <p class="job-title">{{ item.job_title }}</p>
            <p class="bio">{{ item.bio }}</p>
          </div>
        </div>
      {% endfor %}
  </section>
{% endif %}
